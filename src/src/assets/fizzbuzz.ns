def fizzbuzz(n) :
    i = 1
    b = False

    while i <= n :
        if i % 3 == 0 :
            b = True
            print("Fizz")
        end
        if i % 5 == 0 :
            b = True
            print("Buzz")
        end
        if not b :
            print(i)
        end

        print("\n")
        b = False
    end
end

fizzbuzz(100)