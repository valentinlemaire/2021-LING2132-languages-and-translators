def fizzbuzz(n) :
    i = 1
    b = False

    while i <= n :
        if i % 15 == 0:
            println("FizzBuzz")
        elsif i % 3 == 0 :
            println("Fizz")
        elsif i % 5 == 0 :
            println("Buzz")
        else:
            println(i)
        end
         i = i + 1
    end
end

fizzbuzz(100)